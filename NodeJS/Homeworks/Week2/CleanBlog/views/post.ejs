<%- include('partials/_footer'); -%>

<body>

  <%- include('partials/_navigation'); -%>

  <!-- Page Header -->
  <header class="masthead" style="background-image: url('img/post-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="post-heading">
            <h1><%=post.title%></h1>
            <span class="meta">Posted by
              <a href="https://github.com/4lbatr0s" target="_blank">Serhat Oner</a>
              <%=post.dateCreated%></span>
          </div>
        </div>
      </div>
    </div>
  </header>

  <!-- Post Content -->
  <article>
    <div class="container">
      <div class="row"> 
        <div class="col-lg-8 col-md-10 mx-auto">
          <p><%=post.context%></p>
        </div>
      </div>
    </div>
  </article>

  <hr>

  <%- include('partials/_footer'); -%>

 

</body>

</html>
